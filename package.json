{
  "name": "extract-base-iterator",
  "version": "1.0.0",
  "description": "Base iterator for extract iterators like tar-iterator and zip-iterator",
  "keywords": [
    "extract",
    "base",
    "iterator",
    "asyncIterator",
    "Symbol.asyncIterator",
    "tar",
    "tar-terator",
    "zip",
    "zip-iterator"
  ],
  "homepage": "https://github.com/kmalakoff/extract-base-iterator",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/extract-base-iterator.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --apply-unsafe src/ test/",
    "test": "tsds test:node --timeout=10000",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "fs-access-compat": "^1.0.3",
    "graceful-fs": "^4.2.11",
    "is-absolute": "^1.0.0",
    "lodash.compact": "^3.0.1",
    "mkpath": "^1.0.0",
    "queue-cb": "^1.2.1",
    "rimraf": "^2.7.1",
    "stack-base-iterator": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "fs-iterator": "^5.0.1",
    "fs-stats-spys": "^1.0.2",
    "inherits": "^2.0.4",
    "lodash.find": "^4.6.0",
    "ts-dev-stack": "^0.14.9"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
