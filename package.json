    "readable-stream": "^2.3.8",
    "rimraf2": "*",
    "stack-base-iterator": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "cr": "*",
    "fs-iterator": "*",
    "fs-stats-spys": "*",
    "lodash.find": "*",
    "node-version-use": "*",
    "pinkie-promise": "*",
    "ts-dev-stack": "*"
